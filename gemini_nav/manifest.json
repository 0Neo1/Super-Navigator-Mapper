{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Gemini Chat Navigator",
  "version": "1.0",
  "description": "Adds a navigation sidebar to Gemini chats based on user prompts for easier navigation.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://gemini.google.com/app/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
