{
  "manifest_version": 3,
  "name": "Prompt Engine by ZeroEka",
  "version": "1.0.0",
  "description": "Transform your ideas into super-optimized prompts with advanced Prompt Engine by ZeroEka. ",
  "permissions": [
    "sidePanel",
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "https://api.promptengine.zeroeka.com/*",
    "http://13.223.210.234/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "action": {
    "default_title": "Open Prompt Engine",
    "default_icon": {
      "16": "icon-16.png",
      "32": "icon-32.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://www.chatgpt.com/*",
        "https://gemini.google.com/*"
      ],
      "js": ["listener.js"],
      "run_at": "document_end"
    }
  ],
  "externally_connectable": {
    "ids": [
      "gljkcindldfbbnhcbdlojicnamlhjamj"
    ]
  },
  "web_accessible_resources": [{
    "resources": ["login-success.js"],
    "matches": ["<all_urls>"]
  }],
  "author": "ZeroEka",
  "homepage_url": "https://zeroeka.com"
}