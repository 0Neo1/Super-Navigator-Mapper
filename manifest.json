{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "__MSG_name__",
  "version": "1.1.0",
  "description": "__MSG_description__",
  "icons": {
    "16": "images/icons/icon-128.png",
    "32": "images/icons/icon-128.png",
    "48": "images/icons/icon-128.png",
    "64": "images/icons/icon-128.png",
    "128": "images/icons/icon-128.png"
  },
  "default_locale": "en",
  "permissions": [
    "storage",
    "cookies",
    "activeTab",
    "identity",
    "management",
    "downloads"
  ],
  "host_permissions": [
    "https://chatgpt.com/",
    "https://www.chatgpt.com/",
    "https://accounts.google.com/*",
    "https://oauth2.googleapis.com/*",
    "https://identitytoolkit.googleapis.com/*"

    
  ],
  "action": {},
  "oauth2": {
    "client_id": "73293690559-ja3cf3l6g3eqmrhvt2369uu44a38h1g3.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*",
        "https://www.chatgpt.com/*",
        "https://gemini.google.com/*"
      ],
      "js": [
        "auth-gate.js",
        "global-error-capture.js",
        "tools.js",
        "fullscreen.js",
        "content.js"
      ],
      "css": [
        "styles.css"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://chatgptmessagetree.com/pay/*",
        "https://www.chatgptmessagetree.com/pay/*"
      ],
      "js": [
        "content-payment.js"
      ],
              "run_at": "document_end"
      }
    ],
  "web_accessible_resources": [
    {
      "resources": [
        "images/*",
        "resources/*",
        "resources/firebase/*",
        "resources/login.js",
        "jsmind.js",
        "jsmind.css",
        "jsmind.draggable-node.js",
        "resources/mindmap.html",
        "resources/mindmap.js"
      ],
      "matches": [
        "https://chatgpt.com/*",
        "https://www.chatgpt.com/*",
        "https://gemini.google.com/*"
      ]
    }
  ]
}